{
  "name": "eunchurn.com",
  "version": "1.5.1",
  "private": true,
  "scripts": {
    "start": "next start",
    "start:dev": "cross-env SOCKET=true NODE_ENV=dev node ./scripts/next-remote-watch.js ./data",
    "dev": "next dev",
    "build": "next build && npm run -s build:sitemap",
    "build:sitemap": "node ./scripts/generate-sitemap",
    "serve": "next start",
    "export": "cross-env EXPORT=true next export",
    "build:github": "cross-env GITHUB=true next build && npm run -s build:sitemap",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fontsource/inter": "5.0.16",
    "@mailchimp/mailchimp_marketing": "3.0.80",
    "@remark-embedder/core": "3.0.1",
    "@remark-embedder/transformer-oembed": "3.0.0",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "autoprefixer": "10.4.14",
    "esbuild": "0.18.6",
    "github-slugger": "2.0.0",
    "gray-matter": "4.0.3",
    "image-size": "1.0.2",
    "js-yaml": "^4.1.0",
    "mdx-bundler": "9.2.1",
    "next": "13.4.7",
    "next-pwa": "5.6.0",
    "next-themes": "^0.2.1",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-citation": "1.0.1",
    "rehype-katex": "6.0.3",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "1.5.1",
    "rehype-slug": "5.1.0",
    "remark-footnotes": "4.0.1",
    "remark-gfm": "3.0.1",
    "remark-math": "5.1.1",
    "sharp": "0.32.1",
    "smoothscroll-polyfill": "0.4.4",
    "tailwindcss": "3.3.2",
    "unist-util-visit": "4.1.2",
    "vfile": "^5.3.7"
  },
  "devDependencies": {
    "@eunchurn/eslint-config": "^0.1.13",
    "@eunchurn/prettier-config": "^0.0.6",
    "@next/bundle-analyzer": "13.4.7",
    "@svgr/webpack": "8.0.1",
    "@types/js-yaml": "^4.0.5",
    "@types/next-pwa": "^5.6.2",
    "@types/react": "18.2.14",
    "@types/tailwindcss": "3.0.11",
    "@types/unist": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "cross-env": "7.0.3",
    "dedent": "0.7.0",
    "eslint-config-next": "13.4.7",
    "file-loader": "6.2.0",
    "globby": "13.2.0",
    "husky": "8.0.3",
    "inquirer": "9.2.7",
    "lint-staged": "13.2.3",
    "next-compose-plugins": "2.2.1",
    "next-remote-watch": "2.0.0",
    "prettier-plugin-tailwindcss": "0.3.0",
    "pretty-quick": "3.1.3",
    "socket.io": "4.7.1",
    "socket.io-client": "4.7.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@8.6.3"
}