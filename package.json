{
  "name": "eunchurn.com",
  "version": "1.5.1",
  "private": true,
  "scripts": {
    "start": "next start",
    "start:dev": "cross-env SOCKET=true NODE_ENV=dev node ./scripts/next-remote-watch.js ./data",
    "dev": "next dev",
    "build": "next build && npm run -s build:sitemap",
    "build:sitemap": "node ./scripts/generate-sitemap",
    "serve": "next start",
    "export": "cross-env EXPORT=true next export",
    "build:github": "cross-env GITHUB=true next build && npm run -s build:sitemap",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fontsource/inter": "5.0.17",
    "@mailchimp/mailchimp_marketing": "3.0.80",
    "@remark-embedder/core": "3.0.2",
    "@remark-embedder/transformer-oembed": "3.0.0",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "autoprefixer": "10.4.18",
    "esbuild": "0.20.1",
    "github-slugger": "2.0.0",
    "got": "^14.2.1",
    "gray-matter": "4.0.3",
    "image-size": "1.1.1",
    "js-yaml": "^4.1.0",
    "lqip-modern": "^2.0.0",
    "mdx-bundler": "10.0.1",
    "next": "14.1.3",
    "next-pwa": "5.6.0",
    "next-themes": "^0.2.1",
    "notion-client": "^6.16.0",
    "notion-types": "^6.16.0",
    "notion-utils": "^6.16.0",
    "p-map": "^7.0.1",
    "p-memoize": "^7.1.1",
    "postcss": "8.4.35",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-notion-x": "^6.16.0",
    "react-tweet-embed": "^2.0.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "7.1.0",
    "rehype-citation": "2.0.0",
    "rehype-katex": "7.0.0",
    "rehype-preset-minify": "7.0.0",
    "rehype-prism-plus": "2.0.0",
    "rehype-slug": "6.0.0",
    "remark-footnotes": "4.0.1",
    "remark-gfm": "4.0.0",
    "remark-math": "6.0.0",
    "sharp": "0.33.2",
    "smoothscroll-polyfill": "0.4.4",
    "tailwindcss": "3.4.1",
    "unist-util-visit": "5.0.0",
    "vfile": "^6.0.1"
  },
  "devDependencies": {
    "@eunchurn/eslint-config": "^0.1.13",
    "@eunchurn/prettier-config": "^0.0.6",
    "@next/bundle-analyzer": "14.1.3",
    "@svgr/webpack": "8.1.0",
    "@types/got": "^9.6.12",
    "@types/js-yaml": "^4.0.9",
    "@types/next-pwa": "^5.6.9",
    "@types/react": "18.2.64",
    "@types/tailwindcss": "3.0.11",
    "@types/unist": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "cross-env": "7.0.3",
    "dedent": "1.5.1",
    "eslint-config-next": "14.1.3",
    "file-loader": "6.2.0",
    "globby": "14.0.1",
    "husky": "9.0.11",
    "inquirer": "9.2.15",
    "lint-staged": "15.2.2",
    "next-compose-plugins": "2.2.1",
    "next-remote-watch": "2.0.0",
    "prettier-plugin-tailwindcss": "0.5.12",
    "pretty-quick": "4.0.0",
    "socket.io": "4.7.4",
    "socket.io-client": "4.7.4",
    "ts-node": "10.9.2",
    "typescript": "5.4.2"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@8.15.8"
}
